pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                echo 'Built'
            }
        }
        stage('Test') {
            steps {
                echo "Primero Tested"
                echo "Segundo tested"
                echo "Tercero tested"
                echo "Cuarto tested"
  
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying.....';echo 'deployed'
                
            }
        }
    }
}